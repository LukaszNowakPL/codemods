{
  "name": "codemods",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:all": "npm install && npm --prefix consumer install && npm --prefix library install",
    "test:codemods": "npm --prefix library run test",
    "codemods": "npx jscodeshift ./consumer/src/ --parse=tsx --extensions=ts,tsx --transform=./library/codemods/v2/breadcrumbs.js",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "prettier": "^3.0.3"
  }
}
